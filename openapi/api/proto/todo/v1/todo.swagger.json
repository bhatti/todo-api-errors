                },
                "description": {
                  "type": "string",
                  "title": "Task description"
                },
                "status": {
                  "$ref": "#/definitions/todov1Status",
                  "title": "Task status"
                },
                "priority": {
                  "$ref": "#/definitions/v1Priority",
                  "title": "Task priority"
                },
                "dueDate": {
                  "type": "string",
                  "format": "date-time",
                  "title": "Due date for the task"
                },
                "createTime": {
                  "type": "string",
                  "format": "date-time",
                  "title": "Task creation time",
                  "readOnly": true
                },
                "updateTime": {
                  "type": "string",
                  "format": "date-time",
                  "title": "Task last update time",
                  "readOnly": true
                },
                "createdBy": {
                  "type": "string",
                  "title": "User who created the task",
                  "readOnly": true
                },
                "tags": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  },
                  "title": "Tags associated with the task"
                }
              },
              "title": "Task to update",
              "required": [
                "title",
                "status",
                "task"
              ]
            }
          }
        ],
        "tags": [
          "TodoService"
        ]
      }
